<% if @graph %>
  <% if 2 > 16 %>
  size: <%= @graph.size %>

  <br><br>

  name: <%= @graph.name %>

  <br><br>

  to_s: <%= @graph.to_s %>

  <br><br>

  to_uri: <%= @graph.to_uri %>

  <br><br>

  first: <%= @graph.first %>

  <br><br>

  <%= @graph.methods %>

  <br><br>

  data: <%= @graph.data %>

  <br><br>
  
  data.readable?: <%= @graph.data.readable? %>

  <br><br>
  
  data.count: <%= @graph.data.count %>

  <br><br>
  <% end %>

  <%= @graph.dump(:ntriples) %>

  <% if 2 > 16 %>
  data.each_statement:<br>
  <% @graph.data.each_statement do |statement| %>
    <%= debug(statement) %><br>
    <%= statement.subject %><br>
    <%= statement.object.inspect %>
    <br><br>
  <% end %>
  <% end %>

  <br><br>
<% end %>

<%#
@statements.each do |statement, index|
  index
end
%>

<% if @reader %>
  <%= @reader.methods %>

  <br><br>

  <%= @reader.inspect %>

  <br><br>

  readable?: <%= @reader.readable? %>

  <br><br>

  base_uri: <%= @reader.base_uri %>

  <br><br>

  prefixes: <%= @reader.prefixes %>

  <br><br>

  lineno: <%= @reader.lineno %>

  <br><br>

  entries: <%= @reader.entries %>
<% end %>